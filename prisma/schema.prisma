generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  content   String?
  published Boolean @default(false)
}

model RegisteredUsers {
  Name         String?
  SAP_ID       BigInt? @map("SAP ID")
  Course       String?
  Year         String?
  Collage_Mail String? @map("Collage Mail")
  MTC_ID       String? @map("MTC ID")
  id           BigInt  @id(map: "MTC_Users_pkey") @default(autoincrement())
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
